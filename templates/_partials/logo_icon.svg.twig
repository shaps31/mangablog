<!-- KamiManga • Otaku Eye x Enso Kami (mix) -->
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 120 120"
     role="img" aria-label="KamiManga"
     style="--ink:#4B2E83; --accent:#F26A21; height:75px; width:auto; display:inline-block">
    <defs>
        <style>
            /* Encre principale */
            .s { stroke: var(--ink); stroke-width: 6; stroke-linecap: round; stroke-linejoin: round; fill: none; }
            /* Pinceau du cercle Enso (plus épais) */
            .b { stroke: var(--ink); stroke-width: 8; stroke-linecap: round; stroke-linejoin: round; fill: none; }
            /* Remplissages */
            .f { fill: var(--ink); }
            .af { fill: var(--accent); }
            .as { stroke: var(--accent); stroke-width: 8; fill: none; stroke-linecap: round; }
        </style>
    </defs>

    <!-- Enso (cercle pinceau avec ouverture en haut-droite) -->
    <path d="M24 60a36 36 0 1 0 17-31" class="b"/>
    <!-- Petit trait d'accent du pinceau -->
    <path d="M92 20c6 2 12 6 16 10" class="as"/>

    <!-- Œil manga au centre -->
    <path d="M25 60q18-16 35-16t35 16" class="s"/>
    <path d="M25 60q18 16 35 16t35-16" class="s"/>
    <circle cx="60" cy="60" r="10" class="f"/>
    <circle cx="56" cy="56" r="3" fill="#fff"/>

    <!-- Étoile (sparkle) en accent -->
    <path d="M86 36l2 6 6 2-6 2-2 6-2-6-6-2 6-2z" class="af"/>

    <!-- Lignes de vitesse (discrètes) -->
    <path d="M18 38h10M12 46h12" class="s"/>
    <path d="M92 82h10M96 90h12" class="s"/>

    <!-- Sceau '神' (kanji) en bas-droite -->
    <circle cx="94" cy="90" r="10" class="af"/>
    <text x="94" y="94" text-anchor="middle" font-weight="900" font-size="12" fill="#fff">神</text>
</svg>
